{
    "manifest_version": 2,
    "name": "Weather",
    "version": "0.0.1",
    "icons": { "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png" },
        "browser_action": {
            "default_popup": "popup.html"
        },
    "permissions": [
            "storage"
        ],
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": true
      }
}